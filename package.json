{
    "name": "plogger-sdk",
    "version": "1.0.1",
    "description": "Typescript based frontend logging library compatible with multiple transports and JS/TS frameworks",
    "keywords": [
        "logger",
        "logging",
        "log",
        "logging-library",
        "pLogger",
        "javascript-logger",
        "http-logger",
        "console-logger",
        "log-level",
        "log-transport",
        "logging-framework",
        "structured-logging",
        "log-management",
        "nodejs-logging",
        "browser-logger"
    ],
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "npm run clean && tsc && tsc -m es6 --outDir lib-esm && webpack",
        "build:dev": "webpack --mode=development",
        "build:prod": "webpack --mode=production --node-env=production",
        "watch": "webpack --watch",
        "serve": "webpack serve",
        "clean": "shx rm -rf _bundles lib lib-esm"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/perfios/pLogger.git"
    },
    "author": "Perfios Software Solutions Private Limited",
    "license": "GPL-3.0-only",
    "homepage": "https://github.com/perfios/pLogger/blob/main/README.md",
    "devDependencies": {
        "@types/node": "20.0.0",
        "@webpack-cli/generators": "^3.0.1",
        "html-webpack-plugin": "^5.5.0",
        "shx": "^0.3.4",
        "terser-webpack-plugin": "^5.3.7",
        "ts-loader": "^9.4.2",
        "typescript": "^5.0.3",
        "webpack": "^5.77.0",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^4.13.2"
    },
    "main": "./lib/index.js"
}
